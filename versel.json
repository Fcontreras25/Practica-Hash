{
  "version": 2,
  "functions": {
    "api/agregarUsuario.js": {
      "runtime": "nodejs18.x"
    },
    "api/crearCuenta.js": {
      "runtime": "nodejs18.x"
    },
    "api/guardarNuevaContra.js": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
