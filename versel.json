{
  "version": 2,
  "functions": {
    "api/agregarUsuario.js": {
      "runtime": "nodejs18.x"
    },
    "api/crearCuenta.js": {
      "runtime": "nodejs18.x"
    },
    "api/guardarNuevaContra.js": {
      "runtime": "nodejs18.x"
    },
    "api/redireccionar.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes":  [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}